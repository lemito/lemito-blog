{
  "name": "lemitofrontend",
  "version": "1.0.2",
  "private": true,
  "dependencies": {
    "@bem-react/classname": "^1.5.12",
    "@loadable/component": "^5.15.0",
    "@svgr/webpack": "4.3.3",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "axios": "^0.24.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.1",
    "camelcase": "^5.3.1",
